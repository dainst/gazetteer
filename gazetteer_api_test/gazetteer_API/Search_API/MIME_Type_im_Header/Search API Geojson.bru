meta {
  name: Search API Geojson
  type: http
  seq: 1
}

get {
  url: http://localhost:8080/search?q=rotes+meer
  body: none
  auth: inherit
}

params:query {
  q: rotes+meer
}

headers {
  Accept: application/vnd.geo+json
}

assert {
  res.status: eq 200
  res.body.type: eq "FeatureCollection"
  res.body.features: isNotEmpty
}

settings {
  encodeUrl: true
  timeout: 0
}
